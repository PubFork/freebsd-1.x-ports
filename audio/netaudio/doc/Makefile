install.man:: ncdaudio.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(RM) $(DESTDIR)$(MANDIR)/ncdaudio.0
	cat ncdaudio.man | $(NEQN) | $(TBL) | $(NROFF) -man >/tmp/ncdaudio.man
	$(INSTALL) -c $(INSTMANFLAGS) /tmp/ncdaudio.man $(DESTDIR)$(MANDIR)/ncdaudio.0
	$(RM) /tmp/ncdaudio.man

