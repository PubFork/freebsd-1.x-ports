#!/bin/sh
# Make all the Glonti/Samarin Cyrillic fonts, except the Cyrillic cminch.

make-mf cmbizx 10		# bold italic extended
make-mf cmbozx 10		# bold slanted extended
make-mf cmbszx 10		# bold sans extended
make-mf cmbz 10			# bold
make-mf cmbzx 5 6 7 8 9 10 12	# bold extended
make-mf czssq 8			# sans serif ``quotation''
make-mf czssqi 8		# sans serif italic ``quotation''
make-mf cmdszc 10		# demibold sans condensed
make-mf cmrcz 8 9 10		# small caps
make-mf cmrisz 8 9 10 12 17	# italic sans
make-mf cmritz 10		# italic typewriter
make-mf cmriz 7 8 9 10 12	# italic
make-mf cmrotz 10		# slanted typewriter
make-mf cmroz 8 9 10 12		# slanted
make-mf cmrsz 8 9 10 12 17	# sans
make-mf cmrtz 8 9 10 12		# typewriter
make-mf cmruz 10		# unslanted italic
make-mf cmrz 5 6 7 8 9 10 12 17	# regular
