#Setup for TeX
#execute this with "% source texsetup" or include it in your .login file
#to make the settings valid in your shell.
##Fonts
setenv TEXFONTS $(TFM_USER_386BSD):$(TFM_386BSD)
setenv TEXFONTS_SUBDIR $(TFM_USER_386BSD):
setenv VFFONTS $(VF_USER_386BSD):$(VF_386BSD)
setenv PKFONTS $(PK_USER_386BSD):$(PK_386BSD)
setenv XDVIFONTS $(PK_USER_386BSD):$(PK_386BSD)
##TeX
setenv TEXFORMATS $(TEXFORMATS_USER_386BSD):
setenv TEXINPUTS $(TEXINPUTS_USER_386BSD):
setenv TEXINPUTS_SUBDIR $(TEXINPUTS_USER_386BSD):
##METAFONT
setenv MFBASES $(MFBASES_USER_386BSD):
setenv MFINPUTS $(MFINPUTS_USER_386BSD):
setenv MFINPUTS_SUBDIR $(MFINPUTS_USER_386BSD):
#End of setup for TeX
