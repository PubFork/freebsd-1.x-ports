b1.sml:
  {efu F [isi A ist C
          {Osi A [ist B ist D]}]}
  {est F [ifu F ist A]}
bx.sml:
  {est S [ist F {Ost F [ist B]}]}
----------------------------------
b2.sml:
  {esi A}
  {est A [isi A]}
b1.sml:
  {efu F [isi A ist C
          ist B ist D]}
  {est F}
bx.sml:
  {est S [ist F ist B]}
----------------------------------
b3.sml:
  {esi A [est B]}
  {est A [isi A est B]}
b1.sml:
  {efu F [isi A ist C
          est B ist D]}
  {est F [est B]}
bx.sml:
  {est S [ist F]}
----------------------------------
