d1.sml:
  {efu F [isi A ist C
          {Osi A
            [ist B {Ost B [ist D]}]}]}
----------------------------------
d2.sml:
  {esi A}
d1.sml:
  {efu F [isi A ist C
          ist B
          {Ost B [ist D]}]}
----------------------------------
d3.sml:
  {esi A [est B]}
d1.sml:
  {efu F [isi A ist C
          est B {Ost B [ist D]}]}
d1.sml:
  {efu F [isi A ist C
          ist D]}
----------------------------------
d4.sml:
  {esi A [est B est D]}
d1.sml:
  {efu F [isi A ist C
          est B est D {Ost B [ist D]}]}
d1.sml:
  {efu F [isi A ist C
          est B est D]}
----------------------------------
d5.sml:
  {esi A [est B [est D]]}
d1.sml:
  {efu F [isi A ist C
          (est B [est D]) {Ost B [ist D]}]}
d1.sml:
  {efu F [isi A ist C
          (est B [est D]) est D]}
