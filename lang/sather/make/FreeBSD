CC           = gcc
MAINCC       = ${CC}
SDBCC        = ${CC}

MAINFLAGS    = -DGC_ -DSAFE_RT_ -w
CFLAGS       = -O ${MAINFLAGS} -D__FreeBSD__
GCFLAGS      = -O -D__FreeBSD__

TCLIBS       =
MFLAGS       =
BOOTFLAGS    =

SKIPSDB      = YES
