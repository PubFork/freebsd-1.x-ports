--* FUNCTION: Run all tests of the data_structure classes.
--*
--* CLASSES: DATA_STRUCTURE_CLASSES_TEST
--* 
--* REQUIRED FILES: ./*.sa
--*
--* RELATED FILES:
--*
--* HISTORY:
--* Last edited: Mar  8 21:56 1992 (hws)
--* Created: Tue May 28 19:47:05 1991 (hws)
--*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

class DS_TEST is
   -- This puts tests of all the data structure classes into a
   -- single program. If output of this program is redirected to
   -- a file, only a summary of whether each class passed or did
   -- not pass its tests will be seen. See
   -- "sather/etc/test/data_structure_test.sather" for an appropriate
   -- ".sather" file to use.
   
   main is
      -- Run all tests of the data_structure classes.
      BTREE_TEST::main;
      BVEC_TEST::main;
      GAPL_TEST::main;
      INTH_TEST::main;
      LIST_TEST::main;
      OBH_TEST::main;
      PRIOQ_TEST::main;
      QUEUE_TEST::main;
      SORT_TEST::main;
      STACK_TEST::main;
      STRH_TEST::main;
      UNIONF_TEST::main;
   end; -- main

end; -- DS_TEST

--*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
