#!/bin/sh
rm -f ./franz/h/aout.h
ln -s /usr/include/a.out.h ./franz/h/aout.h
rm -f ./franz/h/lispo.h
ln -s /usr/include/a.out.h ./franz/h/lispo.h
rm -f ./franz/h/compiled.h
ln -s ../../cliszt/in-c/compiled.h ./franz/h/compiled.h
rm -f ./lisplib/nld-link
ln -s /usr/bin/ld ./lisplib/nld-link
rm -f ./cliszt/i386
ln -s in-c ./cliszt/i386
rm -f ./lisp
ln -s . ./lisp
rm -f ./liszt
ln -s cliszt ./liszt
