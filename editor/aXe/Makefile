	$(RM) $(PROGRAM)

extension::
	$(RM) $(EXTN_DEPS)
	$(MAKE)

install:: Axe.ad
	@if [ -d $(DESTDIR)${Appdir} ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)${Appdir}); fi
	$(INSTALL) -c $(INSTAPPFLAGS) Axe.ad $(DESTDIR)${Appdir}/Axe

