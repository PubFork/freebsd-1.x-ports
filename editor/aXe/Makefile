	@if [ -d X11/Xaw3d/X11 ]; then set +x; \
	else (set -x; $(MKDIRHIER) X11/Xaw3d/X11); fi
	@(set -x; cd X11/Xaw3d/X11; for i in Xaw Xaw3d; do \
	$(RM) $$i; \
	ln -s Xaw3dIncDir $$i; \
	done)

