SUBDIRS = tclLib

 PROGRAM = point

all:: point

point: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

install:: point
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  point $(DESTDIR)$(BINDIR)

install.man:: point.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) point.man $(DESTDIR)$(MANDIR)/point.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAM)

all::

install::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	@for i in  $(POINT_LIBRARY); do if [ -d $(DESTDIR)$$i ]; then \
	set +x; else (set -x; $(MKDIRHIER) $(DESTDIR)$$i); fi; \
	done

install:: $(TCL_FILES)
	@if [ -d $(DESTDIR) $(POINT_LIBRARY) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR) $(POINT_LIBRARY)); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(TCL_FILES); do \
	(set -x; $(INSTALL) -c  $(INSTDATAFLAGS) $$i $(DESTDIR) $(POINT_LIBRARY)); \
	done

depend::

clean::

pkg: pkg_bin

pkg_bin: install
	pkg_create -c pkg/+ONELINE -d pkg/+DESC -f pkg/+PLIST 		-r pkg/+REQ point_bin

