DEFINES = -DSTANDALONE -DNO_LIB_DESKSET $(XVCLIENTDEFINES)
SYS_LIBRARIES = -lm $(SYSV_CLIENT_LIB)

INCLUDES = -I. -I$(HEADER_DEST) -I$(TOP)  $(XVCLIENTINCLUDES)
DEPLIBS =
LOCAL_LIBRARIES = $(XVIEWLIB) $(OLGXLIB) $(XLIB) $(XVCLIENTMISCLIB) -lrpc -lgnuregex

HEADERS = ds_popup.h
INFOFILES = clock.info
MANPAGES = clock.man
SRCS = clock.c
OBJS = clock.o
ALLFILES = ${HEADERS} ${INFOFILES} ${MANPAGES} ${SRCS}

 PROGRAM = clock

all:: clock

clock: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

install:: clock
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  clock $(DESTDIR)$(BINDIR)

install.man:: clock.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(RM) $(DESTDIR)$(MANDIR)/clock.$(MANSUFFIX)*
	$(INSTALL) -c $(INSTMANFLAGS) clock.man $(DESTDIR)$(MANDIR)/clock.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAM)

# @(#)XView.prog	1.6 28 Jun 1993 SMI

populate::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	-@if [ -h SCCS ]; then \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	elif [ -h SCCS- ]; then \
	$(MV) SCCS- SCCS ; \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	$(MV) SCCS SCCS- ; \
	else \
	echo "SCCS or SCCS- do not exist" ; \
	fi

install::
	$(MKDIRHIER) $(HELP_DEST)

install::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	for i in $(INFOFILES) ;\
	do \
	echo "installing $$i"; \
	$(RM) $(HELP_DEST)/$$i ; \
	$(INSTALL) -c $(INSTDATFLAGS) $$i $(HELP_DEST) ; \
	done

install.srcs::

includes::

install.man::

