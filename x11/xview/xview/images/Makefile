all: $(SRCS)

includes::
	$(MKDIRHIER) $(BUILDINCDIR)/$(IMAGE_INCDIR)
	@(set -x; cd $(BUILDINCDIR)/$(IMAGE_INCDIR); for i in $(INCLUDES_SRCS); do \
	$(RM) $$i; \
	$(LN) $(BUILDINCTOP)//$(CURRENT_DIR)/$$i .; \
	done)

install:: $(SRCS)
	$(MKDIRHIER) $(HEADER_DEST)/$(IMAGEINC)

install:: $(SRCS)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(SRCS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(HEADER_DEST)/$(IMAGEINC)); \
	done

populate::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	-@if [ -h SCCS ]; then \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	elif [ -h SCCS- ]; then \
	$(MV) SCCS- SCCS ; \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	$(MV) SCCS SCCS- ; \
	else \
	echo "SCCS or SCCS- do not exist" ; \
	fi

clean::

depend::

###########################################################################
# common rules for all Makefiles - do not edit

emptyrule::

clean::
	$(RM_CMD) \#*

Makefile:: $(IMAKE)

$(IMAKE):
	@(cd $(IMAKESRC); if [ -f Makefile ]; then \
	echo "checking $@ in $(IMAKESRC) first..."; $(MAKE) all; else \
	echo "bootstrapping $@ from Makefile.ini in $(IMAKESRC) first..."; \
	$(MAKE) -f Makefile.ini BOOTSTRAPCFLAGS=$(BOOTSTRAPCFLAGS); fi; \
	echo "okay, continuing in $(CURRENT_DIR)")

Makefile::
	-@if [ -f Makefile ]; then \
	echo "	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak"; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

saber:
	#load $(ALLDEFINES) $(SRCS)

osaber:
	#load $(ALLDEFINES) $(OBJS)

