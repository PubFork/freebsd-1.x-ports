#define IHaveSubdirs
#define PassCDebugFlags

     SUBDIRS = lib sxpm

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))

pkg: pkg_bin

pkg_bin: pkg/+DESC pkg/+ONELINE pkg/+PLIST all install install.man
	pkg_create -c pkg/+ONELINE -d pkg/+DESC -f pkg/+PLIST xpm_bin

