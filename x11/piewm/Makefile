all:: piewm ssetroot

parse.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) '-DSYSTEM_INIT_FILE="$(PIEWMDIR)/system.twmrc"' $*.c

vdt.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $(XLOADIM_DEF) $*.c

depend:: lex.c gram.c deftwmrc.c

 PROGRAM = piewm

all:: piewm

piewm: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS) $(EXTRA_LOAD_FLAGS)

install:: piewm
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  piewm $(DESTDIR)$(BINDIR)

install.man:: piewm.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) piewm.man $(DESTDIR)$(MANDIR)/piewm.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAM)

ssetroot: ssetroot.o
	$(RM) $@
	$(CC) -o $@ ssetroot.o $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) ssetroot

install::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	@for i in $(PIEWMDIR); do if [ -d $(DESTDIR)$$i ]; then \
	set +x; else (set -x; $(MKDIRHIER) $(DESTDIR)$$i); fi; \
	done

install:: system.twmrc
	$(INSTALL) -c $(INSTDATFLAGS) system.twmrc $(DESTDIR)$(PIEWMDIR)
#
install:: ssetroot
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTBINFLAGS) ssetroot $(DESTDIR)$(BINDIR)/ssetroot

gram.h gram.c: gram.y
	$(YACC) $(YFLAGS) gram.y
	$(MV) y.tab.c gram.c
	$(MV) y.tab.h gram.h

clean::
	$(RM) y.tab.h y.tab.c lex.yy.c gram.h gram.c lex.c deftwmrc.c

deftwmrc.c:  system.twmrc
	$(RM) $@
	@echo '/* ' >>$@
	@echo ' * This file is generated automatically from the default' >>$@
	@echo ' * twm bindings file system.twmrc by the twm Imakefile.' >>$@
	@echo ' */' >>$@
	@echo '' >>$@
	@echo 'char *defTwmrc[] = {' >>$@
	sed -e '/^#/d' -e 's/"/\\"/g' -e 's/^/    "/' -e 's/$$/",/' 		system.twmrc >>$@

	@echo '    (char *) 0 };' >>$@

