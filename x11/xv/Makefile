PROGRAMS= xv bggen vdcomp xcmap

xv:	deplibset

deplibset:
	@echo "  building xv ..."
	@echo ""
	@echo "Making sure that libtiff and libjpeg are built first.."
	@echo ""
	@(cd ../tiff; make all install)
	@(cd ../jpeg; make all install)

SRCS = $(SRCS1) $(SRCS2) $(SRCS3) $(SRCS4)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

all:: $(PROGRAMS)

xv: $(OBJS1) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS1) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) xv

bggen: $(OBJS2) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS2) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) bggen

vdcomp: $(OBJS3) $(DEPLIBS)
	$(RM) $@
	$(CC) -o $@ $(OBJS3) $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

