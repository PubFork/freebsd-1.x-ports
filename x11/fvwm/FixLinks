#!/bin/sh
if [ ! -f fvwm.README ] ; then ; exit 1 ; fi

rm -f configure.h
cp -f configurations/FreeBSD.h configure.h
cd  xpmroot
rm -f xpm.h
cp -f ../fvwm/xpm.h xpm.h
rm -f configure.h
cp -f ../configure.h configure.h
cd ..
cd modules/GoodStuff
rm -f alpha_header.h
rm -f configure.h
rm -f module.h
rm -f sun_headers.h
rm -f version.h
rm -f xpm.h
cp -f ../../fvwm/alpha_header.h alpha_header.h
cp -f ../../configure.h .
cp -f ../../fvwm/module.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../version.h .
cp -f ../../fvwm/xpm.h .
cd  ../FvwmBanner
rm -f alpha_header.h
rm -f configure.h
rm -f sun_headers.h
rm -f version.h
rm -f xpm.h
cp -f ../../configure.h .
cp -f ../../fvwm/xpm.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../fvwm/alpha_header.h .
cd  ../FvwmDebug
rm -f alpha_header.h
rm -f configure.h
rm -f module.h
rm -f sun_headers.h
rm -f version.h
cp -f ../../fvwm/alpha_header.h .
cp -f ../../configure.h .
cp -f ../../fvwm/module.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../version.h .
cd  ../FvwmClean
rm -f alpha_header.h
rm -f configure.h
rm -f module.h
rm -f sun_headers.h
rm -f version.h
cp -f ../../fvwm/alpha_header.h .
cp -f ../../configure.h .
cp -f ../../fvwm/module.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../version.h .
cd ../FvwmSave
rm -f alpha_header.h
rm -f configure.h
rm -f module.h
rm -f sun_headers.h
rm -f version.h
cp -f ../../fvwm/alpha_header.h .
cp -f ../../configure.h .
cp -f ../../fvwm/module.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../version.h .
cd ../FvwmIdent
rm -f alpha_header.h
rm -f configure.h
rm -f module.h
rm -f sun_headers.h
rm -f version.h
cp -f ../../fvwm/alpha_header.h .
cp -f ../../configure.h .
cp -f ../../fvwm/module.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../version.h .
cd  ../FvwmPager
rm -f alpha_header.h
rm -f configure.h
rm -f module.h
rm -f sun_headers.h
rm -f version.h
rm -f fvwm.h
cp -f ../../fvwm/alpha_header.h .
cp -f ../../configure.h .
cp -f ../../fvwm/module.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../version.h .
cp -f ../../fvwm/fvwm.h .
cd ../FvwmWinList
rm -f alpha_header.h
rm -f configure.h
rm -f module.h
rm -f sun_headers.h
rm -f version.h
cp -f ../../fvwm/alpha_header.h .
cp -f ../../configure.h .
cp -f ../../fvwm/module.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../version.h .
cd  ../../optional/FvwmSound
rm -f alpha_header.h
rm -f configure.h
rm -f module.h
rm -f sun_headers.h
rm -f version.h
cp -f ../../fvwm/alpha_header.h .
cp -f ../../configure.h .
cp -f ../../fvwm/module.h .
cp -f ../../fvwm/sun_headers.h .
cp -f ../../version.h .
