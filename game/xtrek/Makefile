PROGRAMS	= xtrek xtrekd
BINDIR		= /usr/X386/bin

# This is where the xtrek.snf font will be installed
XTREKFONTDIR	= /usr/X386/lib/X11/fonts/xtrek

# This is where the configs and ship bitmaps will be installed
XTREKCONFDIR	= /usr/X386/lib/X11/games/xtrek

#DEFINES = -DPROFILE

#CC=gcc
#CDEBUGFLAGS = -O -finline-functions -fstrength-reduce -fomit-frame-pointer
#CDEBUGFLAGS = -O
#LDOPTIONS = -pg
#LDOPTIONS = -Bstatic
CONFIGS	= 	TV.config 	default.config 	fed.config	goony.config 	motd

SHIPBM = 	bomb.b 	dart.b 	f14.b 	fed.b 	hawk.b 	idiot.b 	loon.b 	mike.b 	rom.b

SRCS1	= 	atantab.c	clock.c 	colors.c 	coup.c 	damage.c 	data.c 	death.c 	dmessage.c 	enter.c 	getship.c 	help.c 	icon.c 	inform.c 	input.c 	interface.c 	list.c 	main.c 	minisubs.c 	newwin.c 	parseutil.c 	phaser.c 	planet.c 	rasin.c	redraw.c 	resource.c 	rmove.c 	rotate.c 	sintab.c	smessage.c 	sqrt.c	statline.c 	stats.c 	subdaemon.c 	teleport.c 	torp.c 	util.c 	war.c 	xtparse.c

OBJS1 = 	atantab.o	clock.o 	colors.o 	coup.o 	damage.o 	data.o 	death.o 	dmessage.o 	enter.o 	getship.o 	help.o 	icon.o 	inform.o 	input.o 	interface.o 	list.o 	main.o 	minisubs.o 	newwin.o 	parseutil.o 	phaser.o 	planet.o 	rasin.o	redraw.o 	resource.o 	rmove.o 	rotate.o 	sintab.o	smessage.o 	sqrt.o	statline.o 	stats.o 	subdaemon.o 	teleport.o 	torp.o 	util.o 	war.o 	xtparse.o

SRCS2 = 	xtrek.c

OBJS2 = 	xtrek.o

all:: decl.h $(PROGRAMS)

 OBJS = $(OBJS1) $(OBJS2) $(OBJS3)
 SRCS = $(SRCS1) $(SRCS2) $(SRCS3)

all:: $(PROGRAMS)

xtrekd: $(OBJS1) $(DEPLIBS1)
	$(RM) $@
	$(CC) -o $@ $(LDOPTIONS) $(OBJS1)  -L/usr/local/lib $(XLIB) $(LDLIBS)  -lm $(EXTRA_LOAD_FLAGS)

install:: xtrekd
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  xtrekd $(DESTDIR)$(BINDIR)

install.man:: xtrekd.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) xtrekd.man $(DESTDIR)$(MANDIR)/xtrekd.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAMS)

xtrek: $(OBJS2) $(DEPLIBS2)
	$(RM) $@
	$(CC) -o $@ $(LDOPTIONS) $(OBJS2)  -L/usr/local/lib $(XLIB) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: xtrek
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  xtrek $(DESTDIR)$(BINDIR)

install.man:: xtrek.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) xtrek.man $(DESTDIR)$(MANDIR)/xtrek.$(MANSUFFIX)

# font creation protos in Imake.rules are inappropriate

xtrek.pcf.Z:  xtrek.bdf
	$(FONTC) $(FONTCFLAGS) $? | $(COMPRESS) > $@

FONTARGET = xtrek.bdf

install:: $(FONTARGET)
.if !exists(${XTREKFONTDIR})
	mkdir ${XTREKFONTDIR}
.endif
	$(INSTALL) -c $(FONTARGET) $(XTREKFONTDIR)
	(cd $(XTREKFONTDIR); mkfontdir .)

decl.h:   $(SRCS)
	@grep GLOBAL *.c > decl.temp
	@echo	"/* Machine Generated: DO NOT EDIT */" > decl.h
	@sed -e "s/.*GLOBAL/extern/" -e  "s/(.*)/();/" decl.temp >> decl.h
	@$(RM) decl.temp

install:: install.config

install.config:
.if !exists(${XTREKCONFDIR})
	mkdir ${XTREKCONFDIR}
.endif
	@case '${MFLAGS}' in *[i]*) set +e;; esac; 	for i in $(SHIPBM) $(CONFIGS) ; do 		(cd config; set -x; $(INSTALL) -c $(INSTDATFLAGS) $$i $(XTREKCONFDIR)); 	done

doc:
	@echo Making first pass with latex
	latex xtrekguide.doc
	@echo Making second pass with latex
	latex xtrekguide.doc
	dvi2ps xtrekguide.dvi > xtrekguide.ps

clean::
	rm -f xttoken.c xtparse.c

package: pkg_bin

pkg_bin: install install.man
	pkg_create -c pkg/+ONELINE -d pkg/+DESC -f pkg/+PLIST xtrek_bin

xtparse.o: xttoken.c xtparse.c
xttoken.c: xttoken.l
xtparse.c: xtparse.y

